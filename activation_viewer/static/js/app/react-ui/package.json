
{
  "name": "Activation-Viewer",
  "version": "2.0.0",
  "description": "Activation Viewer",
  "repository": {
    "type": "git",
    "url": "https://github.com/simod/activation-viewer.git"
  },
  "scripts": {
    "createdir": "node createbuild.js",
    "start:debug": "node debug-server.js",
    "start:proxy": "node_modules/.bin/json-http-proxy -p 4000",
    "start": "node_modules/.bin/npm-run-all createdir start:debug",
    "build:home": "node_modules/.bin/browserify -g [envify --NODE_ENV production] -p [minifyify --no-map --exclude node_modules/openlayers/dist/ol.js] home.jsx -o build/home.js",
    "build": "node_modules/.bin/npm-run-all createdir build:home build:composer",
    "build:composer": "node_modules/.bin/browserify -g [envify --NODE_ENV production] -p [minifyify --no-map --exclude node_modules/openlayers/dist/ol.js] composer.jsx -o build/composer.js",
    "test": "eslint home.jsx --ext .jsx,.js",
    "createzip": "node createpackage.js",
    "package": "node_modules/.bin/npm-run-all build createzip"
  },
  "author": "simod",
  "license": "Apache-2.0",
   "dependencies": {
    "boundless-sdk": "^0.10.2",
    "material-ui": "^0.16.4",
    "ol3-cesium": "3.9.4",
    "openlayers": "3.19.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-intl": "^2.1.5",
    "react-tap-event-plugin": "^2.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "pure-render-decorator": "^1.1.1",
    "archiver": "^0.21.0",
    "babel-eslint": "^4.1.6",
    "babel-plugin-react-intl": "2.2.0",
    "fs-extra": "0.26.2",
    "json-http-proxy": "1.0.0-ALPHA-4",
    "npm-run-all": "1.3.1",
    "readline": "1.3.0",
    "replacestream": "4.0.0",
    "boundless-sdk-tools": "^0.9.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "babelify": "^7.3.0",
    "browserify": "13.1.0",
    "chai": "^1.10.0",
    "chai-xml": "^0.3.1",
    "clean-css": "^3.4.6",
    "coveralls": "^2.11.14",
    "cssify": "^0.8.0",
    "dr-frankenstyle-win": "0.2.12",
    "envify": "3.4.0",
    "es6-promise": "^3.0.2",
    "eslint": "1.9.0",
    "eslint-plugin-react": "3.9.0",
    "gitbook-cli": "2.1.2",
    "intl": "^1.0.0",
    "licensify": "^2.2.0",
    "minifyify": "^7.1.0",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": "^1.2.0",
    "mochify": "^2.10.0",
    "mochify-istanbul": "^2.4.1",
    "phantomjs-polyfill-find": "github:ptim/phantomjs-polyfill-find",
    "phantomjs-polyfill-object-assign": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "raf": "^3.2.0",
    "react": "^15.4.0",
    "react-addons-test-utils": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-docgen": "^2.2.0",
    "sinon": "^1.12.2",
    "react-tap-event-plugin": "^2.0.0",
    "rimraf": "^2.5.3",
    "through": "^2.3.8",
    "watchify": "^3.2.0"
  },
  "browserify": {
    "transform": [
      "cssify",
      "babelify"
    ]
  }
